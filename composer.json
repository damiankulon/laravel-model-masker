{
  "name": "damiankulon/laravel-model-masker",
  "type": "library",
  "description": "GDPR friendly database masker for Laravel",
  "keywords": [
    "model",
    "laravel",
    "masker"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Damian Kulon",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "^5.7|^6",
    "illuminate/database": "^5.7|^6"
  },
  "autoload": {
    "psr-4": {
      "LaravelModelMasker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelModelMasker\\ServiceProvider"
      ],
      "aliases": {
        "ModelMasker": "LaravelModelMasker\\Facade"
      }
    }
  },
  "require-dev": {
    "laravel/framework": "5.7.*"
  }
}
